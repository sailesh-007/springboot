<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="wood" class="com.training.Wood">
        <constructor-arg name="name" value="Timber"></constructor-arg>
        <constructor-arg name="type" value="High"></constructor-arg>
    </bean>

    <bean id="ship" class="com.training.Ship" autowire="constructor">
        <constructor-arg name="name" value="titanic"></constructor-arg>
    </bean>

</beans>